package poo;

public class Coche {
	
	//escribir las caracteristicas comunes de un carro
	//atributis
	int ruedas;
	int largo;
	int ancho;
	int motor;
	int peso;
	
	//creación del método constructor 
	//método especial que se encarga de dar un estado inicial al objeto
	
	public Coche() {
		ruedas=4;
		largo=2000;
		ancho=300;
		motor=1600;
		peso=500;
		
	}
	
	

}
